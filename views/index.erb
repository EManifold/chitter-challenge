<form
<% if @user %>
action='/new'
<% end %>

<% if !@user %>
action='/sign-up'
<% end %>

method='get'>
  <input type='submit' name='new-peep' value="New peep">
</form>

<h1>New peeps:</h1>

<ul>
  <% @peeps.reverse.each do |peep| %>
  <div id="peep">
    <li><%= peep.userid %> <%= peep.userid %> <%= peep.content %> <%= peep.time %></li>
  </div>
  <% end %>
</ul>
